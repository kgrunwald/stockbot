{
  "expo": {
    "name": "StockBot",
    "slug": "stockbot",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.doacode.stockbot",
      "supportsTablet": true
    },
    "android": {
      "package": "com.doacode.stockbot"
    },
    "description": "Stock Bot",
    "githubUrl": "https://github.com/kgrunwald/stockbot"
  }
}
